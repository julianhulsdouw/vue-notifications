{
    "arrowParens": "always",
    "importOrder": [
        "<THIRD_PARTY_MODULES>",
        "",
        "^@/(.*?)(.vue)$",
        "",
        "^@/(.*)$",
        "",
        "^./(.*?)(.vue)$"
    ],
    "plugins": [
        "@ianvs/prettier-plugin-sort-imports",
        "prettier-plugin-tailwindcss"
    ],
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "vueIndentScriptAndStyle": true
}